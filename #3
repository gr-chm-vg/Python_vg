import turtle

turtle.shape('turtle')  # Установка формы курсора на "черепаху"
for _ in range(4):      # Цикл для повторения действий 4 раза, по одному на каждую сторону квадрата
    turtle.forward(100) # Двигаемся вперед на 100 единиц
    turtle.left(90)     # Поворот налево на 90 градусов
turtle.done()           # Завершаем рисование
